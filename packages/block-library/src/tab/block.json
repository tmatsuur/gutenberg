{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "core/tab",
	"title": "Tab",
	"category": "design",
	"description": "Single tab within a tabs block.",
	"textdomain": "default",
	"__experimental": true,
	"attributes": {
		"title": {
			"type": "string"
		}
	},
	"parent": [ "core/tabs" ],
	"supports": {
		"html": false,
		"inserter": false,
		"reusable": false
	},
	"usesContext": [ "tabs/activeTab" ],
	"style": "wp-block-tab"
}
